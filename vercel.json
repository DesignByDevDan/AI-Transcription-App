{
  "functions": {
    "api/server.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.py"
    }
  ],
  "builds": [
    {
      "src": "api/server.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": 15000000
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
